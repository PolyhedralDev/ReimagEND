if (north == "false" &&
    east == "false" &&
    south == "false" &&
    west == "false") {

    num random = randomInt(4);
    if (random == 0) {
        block(0, -length, 0, "minecraft:chorus_plant[up=true,north=true]", true);
        structure(0, -length, -1, "hanging_chorus_branch_north", "NONE");
    }
    if (random == 1) {
        block(0, -length, 0, "minecraft:chorus_plant[up=true,east=true]", true);
        structure(1, -length, 0, "hanging_chorus_branch_east", "NONE");
    }
    if (random == 2) {
        block(0, -length, 0, "minecraft:chorus_plant[up=true,south=true]", true);
        structure(0, -length, 1, "hanging_chorus_branch_south", "NONE");
    }
    if (random == 3) {
        block(0, -length, 0, "minecraft:chorus_plant[up=true,west=true]", true);
        structure(-1, -length, 0, "hanging_chorus_branch_west", "NONE");
    }
} else {
    block(0,-length,0,"minecraft:chorus_plant[up=true,north="+north+",east="+east+",south="+south+",west="+west+"]");
}