str north = "false";
str east = "false";
str south = "false";
str west = "false";

str sideNorth = "false";
str sideEast = "false";
str sideSouth = "false";
str sideWest = "false";

bool allow = true;

bool large = true;
if (randomInt(2) == 0) large = false;

num length = 1+randomInt(6);

for (num y = 0; y < length; y = y + 1) {
    if (getBlock(0,-y,0) != "minecraft:air") fail;
    block(0,-y,0,"minecraft:chorus_plant[up=true,down=true]");
}

//north
num sideLength = length + 1+randomInt(6);
if (randomInt(2) == 0 && getBlock(0,-length,-1) == "minecraft:air" && getBlock(0,-sideLength,-1) == "minecraft:air") {
    north = "true";
    allow = true;

    for (num y = length+1; y < sideLength; y = y + 1) {
        if (getBlock(0,-y,-1) != "minecraft:air") {
            allow = false;
            fail;
        }
    }

    block(0,-length,-1,"minecraft:chorus_plant[down=true,south=true]");

    if (allow == true) {
        for (num y = length+1; y < sideLength; y = y + 1) {
         block(0,-y,-1,"minecraft:chorus_plant[up=true,down=true]");
        }
    }


    sideNorth = "false";
    sideEast = "false";
    sideSouth = "false";
    sideWest = "false";

    if (randomInt(2) == 0 && getBlock(0,-sideLength,-2) == "minecraft:air" && large) {
        sideNorth = "true";
        structure(0,-sideLength,-2,"hanging_chorus_branch_north","NONE");
    }

    if (randomInt(2) == 0 && getBlock(1,-sideLength,-1) == "minecraft:air" && large) {
        sideEast = "true";
        structure(1,-sideLength,-1,"hanging_chorus_branch_east","NONE");
    }

    if (randomInt(2) == 0 && getBlock(0,-sideLength,0) == "minecraft:air" && large) {
        sideSouth = "true";
        structure(0,-sideLength,0,"hanging_chorus_branch_south","NONE");
    }

    if (randomInt(2) == 0 && getBlock(-1,-sideLength,-1) == "minecraft:air" && large) {
        sideWest = "true";
        structure(-1,-sideLength,-1,"hanging_chorus_branch_west","NONE");
    }

    if (sideNorth == "false" && sideEast == "false" && sideSouth == "false" && sideWest == "false") {
        block(0,-sideLength,-1,"minecraft:chorus_flower[age=5]");
    } else {
        block(0,-sideLength,-1,"minecraft:chorus_plant[up=true,north="+sideNorth+",east="+sideEast+",south="+sideSouth+",west="+sideWest+"]");
    }
}

